<table>
    <tr>
        <th>Nombre</th>
        <th>Grados de alcohol</th>
        <th>Caducidad</th>
        <th>Tipo</th>
        <th>Acciones</th>
    </tr>
    <% arregloTragos.forEach((trago)=>{ %>
    <tr>
        <td> <%=trago.nombre%> </td>
        <td> <%=trago.gradosAlcohol%> </td>
        <td> <%=trago.fechaCaducidad%> </td>
        <td> <%=trago.precio%> </td>
        <td> <%=trago.tipo%> </td>
        <td>
            <button>Editar</button>
            <button>Eliminar</button>
        </td>
    </tr>
  <% })%>
</table>